{
  "name": "@tfnick/nestjs-pg-cache",
  "version": "1.0.4",
  "description": "NestJS cache module based on @keyv/postgres",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "DEBUG_GUIDE.md"
  ],
  "scripts": {
    "build": "tsc --sourceMap --inlineSources",
    "test": "jest --config=test/jest.config.js --passWithNoTests",
    "test:watch": "jest --config=test/jest.config.js --watch",
    "test:cov": "jest --config=test/jest.config.js --coverage",
    "test:mock": "jest --config=test/jest.config.js test/pg-cache.service.mock.spec.ts",
    "test:service": "jest --config=test/jest.config.js test/pg-cache.service.spec.ts",
    "test:decorators": "jest --config=test/jest.config.js test/decorators.spec.ts",
    "test:module": "jest --config=test/jest.config.js test/pg-cache.module.spec.ts",
    "test:quick": "jest --config=test/jest.config.js --maxWorkers=1",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "format": "prettier --write \"{src,test}/**/*.ts\""
  },
  "keywords": [
    "nestjs",
    "cache",
    "postgres",
    "keyv"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@keyv/postgres": "^2.2.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "keyv": "^5.5.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0"
  }
}
